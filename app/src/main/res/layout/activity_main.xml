                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/stateConfirmed"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="#526BFA"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <com.allureinfosystems.covid19india.IconTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="4dp"
                                android:text="@string/increase"
                                android:textColor="#FF0000"
                                android:textSize="10sp" />

                            <Space
                                android:layout_width="4dp"
                                android:layout_height="wrap_content" />

                            <TextView
                                android:id="@+id/sConfirmedIncr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="4dp"
                                android:text="0"
                                android:textColor="#FF0000"
                                android:textSize="10sp"
                                android:textStyle="bold" />

                        </LinearLayout>
                    </LinearLayout>

                    <Space
                        android:layout_width="4dp"
                        android:layout_height="wrap_content" />
                    <!--State Active Linear Layout-->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="#3CF1EBF1"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:padding="4dp">

                        <com.allureinfosystems.covid19india.IconTextView
                            android:layout_width="50dp"
                            android:layout_height="match_parent"
                            android:text="@string/active"
                            android:textColor="#FF0000" />

                        <Space
                            android:layout_width="wrap_content"
                            android:layout_height="4dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Active"
                            android:textColor="#FF0000"
                            android:textSize="12sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/stateActive"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="#FF0000"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <com.allureinfosystems.covid19india.IconTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="4dp"
                                android:text="@string/increase"
                                android:textColor="#FF0000"
                                android:textSize="10sp" />

                            <Space
                                android:layout_width="4dp"
                                android:layout_height="wrap_content" />

                            <TextView
                                android:id="@+id/sActiveIncr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="4dp"
                                android:text="0"
                                android:textColor="#FF0000"
                                android:textSize="10sp"
                                android:textStyle="bold" />

                        </LinearLayout>
                    </LinearLayout>

                    <Space
                        android:layout_width="4dp"
                        android:layout_height="wrap_content" />
                    <!--State Recovered Linear Layout-->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="#3CF1EBF1"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:padding="4dp">

                        <com.allureinfosystems.covid19india.IconTextView
                            android:layout_width="50dp"
                            android:layout_height="match_parent"
                            android:text="@string/recovered"
                            android:textColor="#00FF00" />

                        <Space
                            android:layout_width="wrap_content"
                            android:layout_height="4dp" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Recovered"
                            android:textColor="#00FF00"
                            android:textSize="12sp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/stateRecovered"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="0"
                            android:textColor="#00FF00"
                            android:textSize="14sp"
                            android:textStyle="bold" />

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <com.allureinfosystems.covid19india.IconTextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="4dp"
                                android:text="@string/increase"
                                android:textColor="#FF0000"
                                android:textSize="10sp" />

                            <Space
                                android:layout_width="4dp"
                                android:layout_height="wrap_content" />

                            <TextView
                                android:id="@+id/sRecoveredIncr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="4dp"
                                android:text="0"
                                android:textColor="#FF0000"
                                android:textSize="10sp"
                                android:textStyle="bold" />

                        </LinearLayout>
                    </LinearLayout>

                    <Space
                        android:layout_width="4dp"
                        android:layout_height="wrap_content" />
                    <!--State Deceased Linear Layout-->
                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:background="#3CF1EBF1"
                        android:gravity="center"
                        android:orientation="vertical"
                        android:padding="4dp">

                        <com.allureinfosystems.covid19india.IconTextView
                            android:layout_width="50dp"
                            android:layout_height="match_parent"
                            android:text="@string/skull"
                            android:textColor="#E3CE12" />

                        <Space
                            android:layout_width="wrap_content"
